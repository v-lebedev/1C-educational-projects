    Перем Книга1, Книга2, Таблица1, Таблица2, Совпадения, Несовпадения;
    
    // Загрузка первой таблицы
    Книга1 = Новый ТабличныйДокумент;
    Книга1.Открыть("C:\..."); // Добавьте путь для первой таблицы
    Таблица1 = Книга1.ПолучитьТаблицу();
    
    // Загрузка второй таблицы
    Книга2 = Новый ТабличныйДокумент;
