    Несовпадения = Новый ТаблицаЗначений;
    Несовпадения.Колонки.Добавить("Имя");
    
    // Сравнение таблиц
    Для каждого Строка1 Из Таблица1 Цикл
        Для каждого Строка2 Из Таблица2 Цикл
            Если Строка1["Имя"] = Строка2["Имя"] Тогда
                Совпадения.Добавить().ЗаполнитьЗначения(Строка1["Имя"], "Имя");
            Иначе
                Несовпадения.Добавить().ЗаполнитьЗначения(Строка1["Имя"], "Имя");
            КонецЕсли;
        КонецЦикла;
    КонецЦикла;
    
    // Вывод результатов
    Сообщить("Совпадения:");
    Для каждого Строка Из Совпадения Цикл
        Сообщить(Строка["Имя"]);
    КонецЦикла;
    
    Сообщить("Несовпадения:");
    Для каждого Строка Из Несовпадения Цикл
        Сообщить(Строка["Имя"]);
    КонецЦикла;
КонецПроцедуры
